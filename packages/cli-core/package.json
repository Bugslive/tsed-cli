{
  "name": "@tsed/cli-core",
  "description": "Build your CLI with TypeScript and Decorators",
  "version": "6.0.0-alpha.8",
  "type": "module",
  "main": "./lib/esm/index.js",
  "source": "./src/index.ts",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
