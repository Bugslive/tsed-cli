{
  "name": "@tsed/cli",
  "version": "1.1.2",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "bin": {
    "tsed": "lib/bin/tsed.js"
  },
  "files": [
    "lib/bin/tsed.js",
    "lib/bin",
    "lib",
    "templates"
  ],
  "description": "CLI to bootstrap your Ts.ED project",
  "keywords": [
    "Ts.ED",
    "cli",
    "typescript",
    "framework",
    "rest",
    "api",
    "express",
    "decorators"
  ],
  "dependencies": {
    "@tsed/cli-core": "1.1.2",
    "@tsed/logger": "^5.4.1",
    "change-case": "4.1.1",
    "tslib": "1.11.1"
  },
  "devDependencies": {
    "@tsed/cli-testing": "1.1.2",
    "@types/change-case": "2.3.1"
  },
  "scripts": {
    "build": "tsc --build tsconfig.compile.json",
    "build:doc": "tsc --build tsconfig.doc.json",
    "start:help": "ts-node -r tsconfig-paths/register src/bin/tsed.ts -h",
    "start:version": "ts-node -r tsconfig-paths/register src/bin/tsed.ts --version",
    "start:init:help": "ts-node -r tsconfig-paths/register src/bin/tsed.ts init -h",
    "start:init:run": "ts-node -r tsconfig-paths/register src/bin/tsed.ts init -r ./.tmp/init/default",
    "start:init:run:name": "ts-node -r tsconfig-paths/register src/bin/tsed.ts init -r ./.tmp/init awesome",
    "start:add:run": "ts-node -r tsconfig-paths/register src/bin/tsed.ts add -r ./.tmp",
    "start:generate:help": "ts-node -r tsconfig-paths/register src/bin/tsed.ts generate -h",
    "start:generate:run": "ts-node -r tsconfig-paths/register src/bin/tsed.ts generate --project-root ./.tmp"
  },
  "engines": {
    "node": ">=8.9"
  },
  "peerDependencies": {}
}